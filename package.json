{
  "name": "volatilx-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:action-center2": "tailwindcss -c tailwind.config.js -i ./action_center2/tailwind.css -o ./static/css/action-center2.css --minify && node scripts/build-action-center2.mjs",
    "build:report-center": "tailwindcss -c tailwind.config.js -i ./report_center_frontend/tailwind.css -o ./static/css/report-center.css --minify && node scripts/build-report-center-frontend.mjs",
    "build:signin": "tailwindcss -c tailwind.config.js -i ./signin_frontend/tailwind.css -o ./static/css/signin.css --minify && node scripts/build-signin-frontend.mjs",
    "build:web": "npm run build:action-center2 && npm run build:report-center && npm run build:signin"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.2",
    "esbuild": "^0.23.0",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.6.3"
  }
}
